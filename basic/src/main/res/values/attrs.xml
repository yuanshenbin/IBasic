<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="IButtom">
        <attr name="i_btn_radius_size" format="dimension" /><!--弧度大小-->
        <!--变线的颜色和宽-->
        <attr name="i_btn_stroke_width" format="dimension" />
        <attr name="i_btn_stroke_color" format="color" />
        <attr name="i_btn_solid_color" format="color" /><!--填充的颜色-->

        <!--按下的颜色-->
        <attr name="i_btn_pressed_color" format="color" />
        <!--正常状态颜色-->
        <attr name="i_btn_normal_color" format="color" />
        <!--不可用的颜色-->
        <attr name="i_btn_enabled_color" format="color" />

        <!--字体颜色-->
        <attr name="i_btn_text_color" format="color" />
        <!--未选中-->
        <!--变线的颜色和宽-->
        <attr name="i_btn_stroke_width_unselected" format="dimension" />
        <attr name="i_btn_stroke_color_unselected" format="color" />
        <attr name="i_btn_solid_color_unselected" format="color" /><!--填充的颜色-->

        <!--按下的颜色-->
        <attr name="i_btn_pressed_color_unselected" format="color" />
        <!--正常状态颜色-->
        <attr name="i_btn_normal_color_unselected" format="color" />
        <!--不可用的颜色-->
        <attr name="i_btn_enabled_color_unselected" format="color" />

        <!--是否默认勾选-->
        <attr name="i_btn_selected" format="boolean"/>

        <!--字体颜色-->
        <attr name="i_btn_text_color_unselected" format="color" />

    </declare-styleable>
    <declare-styleable name="ITextView">
        <attr name="i_tv_radius_size" format="dimension" /><!--弧度大小-->
        <!--变线的颜色和宽-->
        <attr name="i_tv_stroke_width" format="dimension" />
        <attr name="i_tv_stroke_color" format="color" />
        <attr name="i_tv_solid_color" format="color" /><!--填充的颜色-->

        <!--按下的颜色-->
        <attr name="i_tv_pressed_color" format="color" />
        <!--正常状态颜色-->
        <attr name="i_tv_normal_color" format="color" />
        <!--不可用的颜色-->
        <attr name="i_tv_enabled_color" format="color" />

        <!--字体颜色-->
        <attr name="i_tv_text_color" format="color" />

        <!--未选中-->
        <!--变线的颜色和宽-->
        <attr name="i_tv_stroke_width_unselected" format="dimension" />
        <attr name="i_tv_stroke_color_unselected" format="color" />
        <attr name="i_tv_solid_color_unselected" format="color" /><!--填充的颜色-->

        <!--按下的颜色-->
        <attr name="i_tv_pressed_color_unselected" format="color" />
        <!--正常状态颜色-->
        <attr name="i_tv_normal_color_unselected" format="color" />
        <!--不可用的颜色-->
        <attr name="i_tv_enabled_color_unselected" format="color" />

        <!--字体颜色-->
        <attr name="i_tv_text_color_unselected" format="color" />

        <!--是否默认勾选-->
        <attr name="i_tv_selected" format="boolean"/>



    </declare-styleable>
</resources>