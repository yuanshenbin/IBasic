        <attr name="i_normal_color_unselected" format="color" />
        <!--不可用的颜色-->
        <attr name="i_enabled_color_unselected" format="color" />

        <!--是否默认勾选-->
        <attr name="i_selected" format="boolean" />

        <!--字体颜色-->
        <attr name="i_text_color_unselected" format="color" />
                字体粗
