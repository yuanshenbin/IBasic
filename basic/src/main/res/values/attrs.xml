    <declare-styleable name="IButton">
        <attr name="i_radius_size" format="dimension" /><!--弧度大小-->
        <!--变线的颜色和宽-->
        <attr name="i_stroke_width" format="dimension" />
        <attr name="i_stroke_color" format="color" />
        <attr name="i_solid_color" format="color" /><!--填充的颜色-->

        <!--按下的颜色-->
        <attr name="i_pressed_color" format="color" />
        <!--正常状态颜色-->
        <attr name="i_normal_color" format="color" />
        <!--不可用的颜色-->
        <attr name="i_enabled_color" format="color" />

        <!--字体颜色-->
        <attr name="i_text_color" format="color" />
        <!--未选中-->
        <!--变线的颜色和宽-->
        <attr name="i_stroke_width_unselected" format="dimension" />
